<UserControl x:Class="Manager.Views.Demos.DemoDetailsView"
			 xmlns:controls="clr-namespace:MahApps.Metro.Controls;assembly=MahApps.Metro"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
			 xmlns:command="http://www.galasoft.ch/mvvmlight"
			 xmlns:converters="clr-namespace:Manager.Converters"
			 xmlns:mouse="clr-namespace:Manager.Internals.Mouse"
			 xmlns:properties="clr-namespace:Manager.Properties"
			 xmlns:internals="clr-namespace:Manager.Internals"
			 mc:Ignorable="d"
			 Name="DetailsUserControl"
			 d:DesignHeight="720"
			 d:DesignWidth="1280"
			 DataContext="{Binding DemoDetails, Source={StaticResource Locator}}">
	<UserControl.Resources>
		<ResourceDictionary>
			<converters:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" True="Visible"/>
			<converters:MapNameToImageConverter x:Key="MapNameToImageConverter" />
			<converters:BooleanInvertedConverter  x:Key="BooleanInvertedConverter" />
			<converters:ValveDemoToVisibilityConverter x:Key="ValveDemoToVisibilityConverter" />
			<converters:RankToImageConverter x:Key="RankToImageConverter" />
			<converters:DoubleToStringConverter x:Key="DoubleToStringConverter" />
			<converters:StringToDemoStatusConverter x:Key="StringToDemoStatusConverter" />
			<converters:SideToStringConverter x:Key="SideToStringConverter" />
			<converters:RoundTypeToStringConverter x:Key="RoundTypeToStringConverter" />
		</ResourceDictionary>
	</UserControl.Resources>

	<i:Interaction.Triggers>
		<i:EventTrigger EventName="Loaded">
			<command:EventToCommand Command="{Binding WindowLoaded, Mode=OneWay}" />
		</i:EventTrigger>
	</i:Interaction.Triggers>

	<UserControl.InputBindings>
		<KeyBinding Command="{Binding BackToHomeCommand}"
					Gesture="CTRL+B" />
		<KeyBinding Command="{Binding HeatmapCommand}"
					CommandParameter="{Binding Demo}"
					Gesture="CTRL+H" />
		<KeyBinding Command="{Binding GoToKillsCommand}"
					CommandParameter="{Binding Demo}"
					Gesture="CTRL+K" />
		<KeyBinding Command="{Binding AnalyzeDemoCommand}"
					Gesture="CTRL+S" />
		<KeyBinding Command="{Binding DemoList.WatchDemoCommand, Source={StaticResource Locator}}"
					CommandParameter="{Binding Demo}"
					Gesture="CTRL+W" />
		<KeyBinding Command="{Binding ExportDemoToExcelCommand}"
					CommandParameter="{Binding Demo}"
					Gesture="CTRL+X" />
		<KeyBinding Command="{Binding OverviewCommand}"
					CommandParameter="{Binding Demo}"
					Gesture="CTRL+O" />
		<KeyBinding Command="{Binding GoToDemoDamagesCommand}"
					CommandParameter="{Binding Demo}"
					Gesture="CTRL+D" />
		<KeyBinding Command="{Binding GoToDemoFlashbangsCommand}"
					CommandParameter="{Binding Demo}"
					Gesture="CTRL+F" />
		<KeyBinding Command="{Binding ShowRoundCommand}"
					CommandParameter="{Binding SelectedRound.Number}"
					Gesture="CTRL+R" />
		<KeyBinding Command="{Binding ShowDemoStuffsCommand}"
					CommandParameter="{Binding Demo}"
					Gesture="CTRL+G" />
		<KeyBinding Command="{Binding GoToMovie}"
					CommandParameter="{Binding Demo}"
					Gesture="CTRL+M" />
		<KeyBinding Command="{Binding GoToPreviousDemoCommand}"
					CommandParameter="{Binding GoToPreviousDemoCommand}"
					Gesture="CTRL+LEFT" />
		<KeyBinding Command="{Binding GoToNextDemoCommand}"
					CommandParameter="{Binding GoToNextDemoCommand}"
					Gesture="CTRL+RIGHT" />
		<mouse:MouseBindingExtended Gesture="XButton1"
									Command="{Binding BackToHomeCommand}"/>
		<mouse:MouseBindingExtended Gesture="XButton2"
									Command="{Binding DemoList.ShowLastUserControlCommand, Source={StaticResource Locator}}"
									CommandParameter="{Binding ElementName=DetailsUserControl}"/>
	</UserControl.InputBindings>

	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="Auto" />
			<RowDefinition Height="Auto" />
			<RowDefinition Height="Auto" />
			<RowDefinition Height="*" />
		</Grid.RowDefinitions>

		<Grid Grid.Row="0">
			<Grid.RowDefinitions>
				<RowDefinition Height="Auto"/>
				<RowDefinition Height="Auto"/>
			</Grid.RowDefinitions>

			<Grid Grid.Row="0"
				  Height="40">
				<StackPanel Orientation="Horizontal">
					<Button Style="{DynamicResource GeneralButton}"
							ToolTip="{x:Static properties:Resources.ToolTipBack}" 
							Command="{Binding BackToHomeCommand}">
						<StackPanel Style="{StaticResource GeneralButtonStackPanel}">
							<Rectangle Width="15"
									   Height="15"
									   Fill="{StaticResource IdealForegroundColorBrush}">
								<Rectangle.OpacityMask>
									<VisualBrush Visual="{StaticResource appbar_arrow_left}"
												 Stretch="Fill" />
								</Rectangle.OpacityMask>
							</Rectangle>
							<TextBlock Text="{x:Static properties:Resources.Back}"
									   Style="{StaticResource ButtonTitleStyle}" />
						</StackPanel>
					</Button>
					<Button Command="{Binding AnalyzeDemoCommand}"
							Style="{DynamicResource GeneralButton}"
							ToolTip="{x:Static properties:Resources.ToolTipAnalyze}">
						<StackPanel Style="{StaticResource GeneralButtonStackPanel}">
							<Rectangle Width="15"
									   Height="15"
									   Fill="{StaticResource IdealForegroundColorBrush}">
								<Rectangle.OpacityMask>
									<VisualBrush Visual="{StaticResource appbar_magnify}"
												 Stretch="Fill" />
								</Rectangle.OpacityMask>
							</Rectangle>
							<TextBlock Text="{x:Static properties:Resources.Analyze}"
									   Style="{StaticResource ButtonTitleStyle}" />
						</StackPanel>
					</Button>
					<Button Command="{Binding DemoList.WatchDemoCommand, Source={StaticResource Locator}}"
							CommandParameter="{Binding Demo}"
							Style="{DynamicResource GeneralButton}"
							ToolTip="{x:Static properties:Resources.ToolTipWatchDemo}">
						<StackPanel Style="{StaticResource GeneralButtonStackPanel}">
							<Rectangle Width="15"
									   Height="15"
									   Fill="{StaticResource IdealForegroundColorBrush}">
								<Rectangle.OpacityMask>
									<VisualBrush Visual="{StaticResource appbar_control_play}"
												 Stretch="Fill" />
								</Rectangle.OpacityMask>
							</Rectangle>
							<TextBlock Text="{x:Static properties:Resources.Watch}"
									   Style="{StaticResource ButtonTitleStyle}" />
						</StackPanel>
					</Button>
					<Button Command="{Binding DemoDetails.WatchRoundCommand, Source={StaticResource Locator}}"
							CommandParameter="{Binding SelectedRound}"
							Style="{DynamicResource GeneralButton}"
							ToolTip="{x:Static properties:Resources.ToolTipWatchRound}">
						<StackPanel Style="{StaticResource GeneralButtonStackPanel}">
							<Rectangle Width="15"
									   Height="15"
									   Fill="{StaticResource IdealForegroundColorBrush}">
								<Rectangle.OpacityMask>
									<VisualBrush Visual="{StaticResource appbar_control_play}"
												 Stretch="Fill" />
								</Rectangle.OpacityMask>
							</Rectangle>
							<TextBlock Text="{x:Static properties:Resources.WatchRound}"
									   Style="{StaticResource ButtonTitleStyle}" />
						</StackPanel>
					</Button>
					<Button Command="{Binding ExportDemoToExcelCommand}"
							Style="{DynamicResource GeneralButton}"
							ToolTip="{x:Static properties:Resources.ToolTipExportXls}">
						<StackPanel Style="{StaticResource GeneralButtonStackPanel}">
							<Rectangle Width="15"
									   Height="15"
									   Fill="{StaticResource IdealForegroundColorBrush}">
								<Rectangle.OpacityMask>
									<VisualBrush Visual="{StaticResource appbar_office_excel}"
												 Stretch="Fill" />
								</Rectangle.OpacityMask>
							</Rectangle>
							<TextBlock Text="{x:Static properties:Resources.Export}"
									   Style="{StaticResource ButtonTitleStyle}" />
						</StackPanel>
					</Button>

					<Button Command="{Binding ExportChatCommand}"
							Style="{DynamicResource GeneralButton}"
							ToolTip="{x:Static properties:Resources.ToolTipExportChat}">
						<StackPanel Style="{StaticResource GeneralButtonStackPanel}">
							<Rectangle Width="15"
									   Height="15"
									   Fill="{StaticResource IdealForegroundColorBrush}">
								<Rectangle.OpacityMask>
									<VisualBrush Visual="{StaticResource appbar_chat}"
												 Stretch="Fill" />
								</Rectangle.OpacityMask>
							</Rectangle>
							<TextBlock Text="{x:Static properties:Resources.ExportChat}"
									   Style="{StaticResource ButtonTitleStyle}" />
						</StackPanel>
					</Button>

					<Button Command="{Binding GoToMovie}"
							CommandParameter="{Binding Demo}"
							Style="{DynamicResource GeneralButton}"
							ToolTip="{x:Static properties:Resources.ToolTipMovie}">
						<StackPanel Style="{StaticResource GeneralButtonStackPanel}">
							<Rectangle Width="15"
									   Height="15"
									   Fill="{StaticResource IdealForegroundColorBrush}">
								<Rectangle.OpacityMask>
									<VisualBrush Visual="{StaticResource appbar_movie_clapper}"
												 Stretch="Fill" />
								</Rectangle.OpacityMask>
							</Rectangle>
							<TextBlock Text="{x:Static properties:Resources.Movie}"
									   Style="{StaticResource ButtonTitleStyle}" />
						</StackPanel>
					</Button>

					<TextBlock Text="{x:Static properties:Resources.LabelPlayerStats}"
							   Style="{StaticResource CurrentFolderStyle}"/>
					<ComboBox Style="{StaticResource ComboBoxAccountsStyle}"
							  ItemsSource="{Binding Demo.Players}"
							  SelectedItem="{Binding SelectedPlayerStats}"
							  DisplayMemberPath ="Name"
							  SelectedValuePath="SteamId"
							  IsEnabled="{Binding Source={StaticResource Locator}, Path=Settings.IsShowAllPlayers, Converter={StaticResource BooleanInvertedConverter}}"
							  controls:TextBoxHelper.Watermark="{x:Static properties:Resources.Player}"/>
					<CheckBox x:Name="CheckBoxShowAllPlayers"
							  Content="{x:Static properties:Resources.ShowAll}"
							  Margin="5"
							  IsChecked="{Binding Source={StaticResource Locator}, Path=Settings.IsShowAllPlayers}"
							  Command="{Binding ShowAllPlayersCommand}"
							  CommandParameter="{Binding IsChecked, ElementName=CheckBoxShowAllPlayers}"/>

				</StackPanel>

				<StackPanel Orientation="Horizontal"
							HorizontalAlignment="Right">
					<Button Style="{DynamicResource GeneralButton}"
							ToolTip="{x:Static properties:Resources.ToolTipPreviousDemo}"
							Command="{Binding GoToPreviousDemoCommand}">
						<StackPanel Style="{StaticResource GeneralButtonStackPanel}">
							<Rectangle Width="15"
									   Height="15"
									   Fill="{StaticResource IdealForegroundColorBrush}">
								<Rectangle.OpacityMask>
									<VisualBrush Visual="{StaticResource appbar_arrow_left}"
												 Stretch="Fill" />
								</Rectangle.OpacityMask>
							</Rectangle>
						</StackPanel>
					</Button>

					<Button Style="{DynamicResource GeneralButton}"
							ToolTip="{x:Static properties:Resources.ToolTipNextDemo}"
							Command="{Binding GoToNextDemoCommand}">
						<StackPanel Style="{StaticResource GeneralButtonStackPanel}">
							<Rectangle Width="15"
									   Height="15"
									   Fill="{StaticResource IdealForegroundColorBrush}">
								<Rectangle.OpacityMask>
									<VisualBrush Visual="{StaticResource appbar_arrow_right}"
												 Stretch="Fill" />
								</Rectangle.OpacityMask>
							</Rectangle>
						</StackPanel>
					</Button>
				</StackPanel>
			</Grid>
		</Grid>

		<Grid Grid.Row="1"
			  Height="40">
			<StackPanel Orientation="Horizontal">
				<Button Command="{Binding HeatmapCommand}"
						CommandParameter="{Binding Demo}"
						Style="{DynamicResource GeneralButton}"
						ToolTip="{x:Static properties:Resources.ToolTipHeatmap}">
					<StackPanel Style="{StaticResource GeneralButtonStackPanel}">
						<Rectangle Width="15"
								   Height="15"
								   Fill="{StaticResource IdealForegroundColorBrush}">
							<Rectangle.OpacityMask>
								<VisualBrush Visual="{StaticResource appbar_map}"
											 Stretch="Fill" />
							</Rectangle.OpacityMask>
						</Rectangle>
						<TextBlock Text="{x:Static properties:Resources.Heatmap}"
								   Style="{StaticResource ButtonTitleStyle}" />
					</StackPanel>
				</Button>

				<Button Command="{Binding OverviewCommand}"
						CommandParameter="{Binding Demo}"
						Style="{DynamicResource GeneralButton}"
						ToolTip="{x:Static properties:Resources.ToolTipOverview}">
					<StackPanel Style="{StaticResource GeneralButtonStackPanel}">
						<Rectangle Width="15"
								   Height="15"
								   Fill="{StaticResource IdealForegroundColorBrush}">
							<Rectangle.OpacityMask>
								<VisualBrush Visual="{StaticResource appbar_map_treasure}"
											 Stretch="Fill" />
							</Rectangle.OpacityMask>
						</Rectangle>
						<TextBlock Text="{x:Static properties:Resources.Overview}"
								   Style="{StaticResource ButtonTitleStyle}" />
					</StackPanel>
				</Button>

				<Button Command="{Binding GoToKillsCommand}"
						CommandParameter="{Binding Demo}"
						Style="{DynamicResource GeneralButton}"
						ToolTip="{x:Static properties:Resources.ToolTipKillsDetails}">
					<StackPanel Orientation="Horizontal">
						<Rectangle Width="25"
								   Height="15"
								   Fill="{StaticResource IdealForegroundColorBrush}">
							<Rectangle.OpacityMask>
								<ImageBrush ImageSource="/resources/images/suicide-black.png" />
							</Rectangle.OpacityMask>
						</Rectangle>
						<TextBlock Text="{x:Static properties:Resources.Kills}"
								   Style="{StaticResource ButtonTitleStyle}"/>
					</StackPanel>
				</Button>

				<Button Command="{Binding GoToDemoDamagesCommand}"
						CommandParameter="{Binding Demo}"
						Style="{DynamicResource GeneralButton}"
						ToolTip="{x:Static properties:Resources.ToolTipDamages}">
					<StackPanel Style="{StaticResource GeneralButtonStackPanel}">
						<Rectangle Width="15"
								   Height="15"
								   Fill="{StaticResource IdealForegroundColorBrush}">
							<Rectangle.OpacityMask>
								<VisualBrush Visual="{StaticResource appbar_medical_pulse}"
											 Stretch="Fill" />
							</Rectangle.OpacityMask>
						</Rectangle>
						<TextBlock Text="{x:Static properties:Resources.Damages}"
								   Style="{StaticResource ButtonTitleStyle}" />
					</StackPanel>
				</Button>

				<Button Command="{Binding GoToDemoFlashbangsCommand}"
						CommandParameter="{Binding Demo}"
						Style="{DynamicResource GeneralButton}"
						ToolTip="{x:Static properties:Resources.ToolTipFlashbangs}">
					<StackPanel Style="{StaticResource GeneralButtonStackPanel}">
						<Rectangle Width="15"
								   Height="15"
								   Fill="{StaticResource IdealForegroundColorBrush}">
							<Rectangle.OpacityMask>
								<VisualBrush Visual="{StaticResource appbar_camera_flash}"
											 Stretch="Fill" />
							</Rectangle.OpacityMask>
						</Rectangle>
						<TextBlock Text="{x:Static properties:Resources.Flashbangs}"
								   Style="{StaticResource ButtonTitleStyle}" />
					</StackPanel>
				</Button>

				<Button Command="{Binding ShowDemoStuffsCommand}"
						CommandParameter="{Binding Demo}"
						Style="{DynamicResource GeneralButton}"
						ToolTip="{x:Static properties:Resources.ToolTipStuffs}">
					<StackPanel Orientation="Horizontal"
								VerticalAlignment="Center"
								Margin="5 0 5 0">
						<Rectangle Width="15"
								   Height="20"
								   Fill="{StaticResource IdealForegroundColorBrush}">
							<Rectangle.OpacityMask>
								<ImageBrush ImageSource="pack://application:,,,/csgodm.resources;component/images/icons/smokegrenade_black.png"
											Stretch="Fill"/>
							</Rectangle.OpacityMask>
						</Rectangle>
						<TextBlock Text="{x:Static properties:Resources.Stuffs}"
								   Style="{StaticResource ButtonTitleStyle}" />
					</StackPanel>
				</Button>

				<Button Command="{Binding ShowRoundCommand}"
						CommandParameter="{Binding SelectedRound.Number}"
						Style="{DynamicResource GeneralButton}"
						ToolTip="{x:Static properties:Resources.ToolTipRoundDetails}">
					<StackPanel Style="{StaticResource GeneralButtonStackPanel}">
						<Rectangle Width="15"
								   Height="15"
								   Fill="{StaticResource IdealForegroundColorBrush}">
							<Rectangle.OpacityMask>
								<VisualBrush Visual="{StaticResource appbar_page_text}"
											 Stretch="Fill" />
							</Rectangle.OpacityMask>
						</Rectangle>
						<TextBlock Text="{x:Static properties:Resources.RoundDetails}"
								   Style="{StaticResource ButtonTitleStyle}" />
					</StackPanel>
				</Button>

				<Button Command="{Binding ShowPlayerCommand}"
						CommandParameter="{Binding SelectedPlayer}"
						Style="{DynamicResource GeneralButton}"
						ToolTip="{x:Static properties:Resources.ToolTipPlayerDetails}">
					<StackPanel Style="{StaticResource GeneralButtonStackPanel}">
						<Rectangle Width="15"
								   Height="15"
								   Fill="{StaticResource IdealForegroundColorBrush}">
							<Rectangle.OpacityMask>
								<VisualBrush Visual="{StaticResource appbar_user}"
											 Stretch="Fill" />
							</Rectangle.OpacityMask>
						</Rectangle>
						<TextBlock Text="{x:Static properties:Resources.PlayerDetails}"
								   Style="{StaticResource ButtonTitleStyle}" />
					</StackPanel>
				</Button>
			</StackPanel>
		</Grid>
		<!-- end action bar -->

		<Grid Grid.Row="2"
			  Visibility="{Binding HasNotification, Converter={StaticResource BooleanToVisibilityConverter}}">
			<StackPanel Orientation="Horizontal"
						HorizontalAlignment="Center"
						Margin="5">
				<controls:ProgressRing IsActive="{Binding IsBusy}"
									   Foreground="{DynamicResource BlackColorBrush}"
									   Width="40"
									   Height="40" />
				<TextBlock Text="{Binding Notification}"
						   Style="{StaticResource NotificationMessageStyle}"
						   Visibility="{Binding HasNotification, Converter={StaticResource BooleanToVisibilityConverter}}"/>
			</StackPanel>
		</Grid>

		<!--Details-->
		<Grid Grid.Row="3">
			<Grid.ColumnDefinitions>
				<ColumnDefinition Width="Auto" />
				<ColumnDefinition Width="*" />
			</Grid.ColumnDefinitions>

			<ScrollViewer Margin="0 5 0 0">
				<Grid>
					<Grid.ColumnDefinitions>
						<ColumnDefinition Width="Auto" />
						<ColumnDefinition Width="Auto" />
					</Grid.ColumnDefinitions>
					<Grid Grid.Column="0">
						<Button Height="400"
								Margin="2"
								Width="8"
								Command="{Binding ToggleLeftSideCommand}"
								Style="{DynamicResource GeneralButton}" />
					</Grid>

					<Grid Grid.Column="1"
						  Margin="5 0 0 0"
						  Visibility="{Binding IsLeftSideVisible, Converter={StaticResource BooleanToVisibilityConverter}}">
						<Grid.RowDefinitions>
							<RowDefinition Height="Auto" />
							<RowDefinition Height="*" />
						</Grid.RowDefinitions>

						<StackPanel Orientation="Vertical"
									Grid.Row="0">
							<Border BorderThickness="1"
									BorderBrush="{DynamicResource AccentColorBrush}">
								<Image Source="{Binding Demo, Converter={StaticResource MapNameToImageConverter}}"
									   Stretch="Fill"
									   MaxHeight="250"/>
							</Border>
							<Grid>
								<Grid.ColumnDefinitions>
									<ColumnDefinition Width="Auto" />
									<ColumnDefinition Width="*" />
								</Grid.ColumnDefinitions>

								<TextBlock Text="{x:Static properties:Resources.LabelFilename}"
										   Grid.Column="0"
										   Style="{StaticResource TextBlockHighlight}">
									<Run Text=" "/>
								</TextBlock>
								<TextBlock Text="{Binding Demo.Name}"
										   Grid.Column="1"
										   Style="{StaticResource TextBlockSmall}"/>
							</Grid>

							<Grid>
								<Grid.ColumnDefinitions>
									<ColumnDefinition Width="Auto" />
									<ColumnDefinition Width="*" />
								</Grid.ColumnDefinitions>

								<TextBlock Text="{x:Static properties:Resources.LabelMap}"
										   Grid.Column="0"
										   Style="{StaticResource TextBlockHighlight}">
									<Run Text=" "/>
								</TextBlock>
								<TextBlock Text="{Binding Demo.MapName}"
										   Grid.Column="1"
										   Style="{StaticResource TextBlockSmall}"/>
							</Grid>

							<Grid>
								<Grid.ColumnDefinitions>
									<ColumnDefinition Width="Auto" />
									<ColumnDefinition Width="*" />
								</Grid.ColumnDefinitions>

								<TextBlock Text="{x:Static properties:Resources.LabelType}"
										   Grid.Column="0"
										   Style="{StaticResource TextBlockHighlight}">
									<Run Text=" "/>
								</TextBlock>
								<TextBlock Text="{Binding Demo.Type}"
										   Grid.Column="1"
										   Style="{StaticResource TextBlockSmall}"/>
							</Grid>

							<Grid>
								<Grid.ColumnDefinitions>
									<ColumnDefinition Width="Auto" />
									<ColumnDefinition Width="*" />
								</Grid.ColumnDefinitions>

								<TextBlock Text="{x:Static properties:Resources.LabelHostname}"
										   Grid.Column="0"
										   Style="{StaticResource TextBlockHighlight}">
									<Run Text=" "/>
								</TextBlock>
								<TextBlock Text="{Binding Demo.Hostname}"
										   Grid.Column="1"
										   Style="{StaticResource TextBlockSmall}"/>
							</Grid>

							<Grid>
								<Grid.ColumnDefinitions>
									<ColumnDefinition Width="Auto" />
									<ColumnDefinition Width="*" />
								</Grid.ColumnDefinitions>

								<TextBlock Text="{x:Static properties:Resources.LabelClientname}"
										   Grid.Column="0"
										   Style="{StaticResource TextBlockHighlight}">
									<Run Text=" "/>
								</TextBlock>
								<TextBlock Text="{Binding Demo.ClientName}"
										   Grid.Column="1"
										   Style="{StaticResource TextBlockSmall}"/>
							</Grid>

							<Grid>
								<Grid.ColumnDefinitions>
									<ColumnDefinition Width="Auto" />
									<ColumnDefinition Width="*" />
								</Grid.ColumnDefinitions>

								<TextBlock Text="{x:Static properties:Resources.LabelTickrate}"
										   Grid.Column="0"
										   Style="{StaticResource TextBlockHighlight}">
									<Run Text=" "/>
								</TextBlock>
								<TextBlock Text="{Binding Demo.ServerTickrate}"
										   Grid.Column="1"
										   Style="{StaticResource TextBlockSmall}"/>
							</Grid>

							<Grid Margin="0 2 0 2">
								<Grid.ColumnDefinitions>
									<ColumnDefinition Width="Auto" />
									<ColumnDefinition Width="Auto" />
								</Grid.ColumnDefinitions>
								<TextBlock Grid.Column="0"
										   Text="{x:Static properties:Resources.Source}"
										   Style="{StaticResource TextBlockHighlight}">
									<Run Text=" "/>
								</TextBlock>
								<ComboBox x:Name="SourceList"
										  Grid.Column="1"
										  ItemsSource="{Binding Source={StaticResource Locator}, Path=DemoDetails.Sources}"
										  SelectedValue="{Binding Demo.SourceName, Mode=OneWay}"
										  DisplayMemberPath="Label"
										  SelectedValuePath="Name"
										  IsEnabled="{Binding IsBusy, Converter={StaticResource BooleanInvertedConverter}}">
									<i:Interaction.Triggers>
										<i:EventTrigger EventName="SelectionChanged">
											<i:InvokeCommandAction Command="{Binding Source={StaticResource Locator}, Path=DemoDetails.SetDemoSourceCommand}"
																   CommandParameter="{Binding ElementName=SourceList, Path=SelectedItem}"/>
										</i:EventTrigger>
									</i:Interaction.Triggers>
								</ComboBox>
							</Grid>

							<Grid Margin="0 2 0 2">
								<Grid.ColumnDefinitions>
									<ColumnDefinition Width="Auto" />
									<ColumnDefinition Width="Auto" />
								</Grid.ColumnDefinitions>
								<TextBlock Grid.Column="0"
										   Text="{x:Static properties:Resources.LabelStatus}"
										   Style="{StaticResource TextBlockHighlight}">
									<Run Text=" "/>
								</TextBlock>
								<ComboBox x:Name="Statuses"
										  Grid.Column="1"
										  ItemsSource="{Binding Source={StaticResource Locator}, Path=Settings.StatusList}"
										  SelectedItem="{Binding Demo.Status, Converter={StaticResource StringToDemoStatusConverter}, Mode=OneTime}"
										  DisplayMemberPath="Label"
										  SelectedValuePath="Name">
									<i:Interaction.Triggers>
										<i:EventTrigger EventName="SelectionChanged">
											<i:InvokeCommandAction Command="{Binding UpdateDemoStatus}" CommandParameter="{Binding ElementName=Statuses, Path=SelectedItem}" />
										</i:EventTrigger>
									</i:Interaction.Triggers>
								</ComboBox>
							</Grid>

							<Grid>
								<Grid.RowDefinitions>
									<RowDefinition Height="Auto" />
									<RowDefinition Height="Auto" />
									<RowDefinition Height="*" />
								</Grid.RowDefinitions>
								<TextBlock Text="{x:Static properties:Resources.CommentLabel}"
										   Grid.Row="0"
										   Style="{StaticResource TextBlockHighlight}"
										   VerticalAlignment="Bottom">
									<Run Text=" "/>
								</TextBlock>
								<TextBox Grid.Row="1"
										 x:Name="TextBoxComment"
										 TextWrapping="Wrap"
										 AcceptsReturn="True"
										 Text="{Binding Demo.Comment}" />
								<Button Grid.Row="2"
										HorizontalAlignment="Center"
										Command="{Binding SaveCommentDemoCommand}"
										CommandParameter="{Binding ElementName=TextBoxComment, Path=Text}"
										Style="{DynamicResource GeneralButton}"
										Content="{x:Static properties:Resources.SaveComment}" />
							</Grid>
						</StackPanel>

						<Grid Grid.Row="1">
							<StackPanel Orientation="Vertical">
								<StackPanel Orientation="Horizontal">
									<Image Source="/Resources/images/headshot.png"
										   Width="50"
										   Height="30"/>
									<TextBlock Text="{x:Static properties:Resources.LabelMostHsPercent}"
											   Style="{StaticResource TextBlockHighlight}">
										<Run Text=" "/>
									</TextBlock>
									<TextBlock Text="{Binding Demo.MostHeadshotPlayer.Name}"
											   Style="{StaticResource TextBlockSmall}" />
								</StackPanel>
								<StackPanel Orientation="Horizontal"
											Margin="0 5 0 0">
