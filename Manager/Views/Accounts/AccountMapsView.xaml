						</telerik:BarSeries>
						<telerik:BarSeries ItemsSource="{Binding NukePercentDatas}"
										   CategoryBinding="Label"
										   ValueBinding="Value"
										   ShowLabels="True">
							<telerik:BarSeries.LabelDefinitions>
								<telerik:ChartSeriesLabelDefinition HorizontalAlignment="Center"
																	Format="{} {0:F1}%"/>
